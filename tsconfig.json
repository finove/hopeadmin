{
    "compilerOptions": {
      "strict": true,
      "module": "ESNext",
      "target": "ESNext",
      "jsx": "preserve",
      "esModuleInterop": true,
      "sourceMap": true,
      "allowJs": false,
      "lib": [
        "es6",
        "dom"
      ],
      "baseUrl": "./",
      "rootDir": "./",
      "moduleResolution": "node",
      "jsxImportSource": "solid-js",
      "types": [
        "solid-js",
        "solid-js/web"
      ]
    }
  }